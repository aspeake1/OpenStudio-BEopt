  <class_name>UtilityBillCalculationsSimple</class_name>
  <display_name>Calculate Simple Utility Bills</display_name>
  <description>Calculate utility bills using a simple method.</description>
  <modeler_description>Calculate utility bills based on user-entered fixed charges and marginal rates. If 'auto' is selected for marginal rates, the state average is used.</modeler_description>
  <arguments>
    <argument>
      <name>elec_fixed</name>
      <display_name>Electricity: Fixed Charge</display_name>
      <description>Monthly fixed charge for electricity.</description>
      <type>String</type>
      <units>$/month</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>8.0</default_value>
    </argument>
    <argument>
      <name>elec_rate</name>
      <display_name>Electricity: Marginal Rate</display_name>
      <description>Price per kilowatt-hour for electricity.</description>
      <type>String</type>
      <units>$/kWh</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>auto</default_value>
    </argument>
    <argument>
      <name>ng_fixed</name>
      <display_name>Natural Gas: Fixed Charge</display_name>
      <description>Monthly fixed charge for natural gas.</description>
      <type>String</type>
      <units>$/month</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>8.0</default_value>
    </argument>
    <argument>
      <name>ng_rate</name>
      <display_name>Natural Gas: Marginal Rate</display_name>
      <description>Price per therm for natural gas.</description>
      <type>String</type>
      <units>$/therm</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>auto</default_value>
    </argument>
    <argument>
      <name>oil_rate</name>
      <display_name>Oil: Marginal Rate</display_name>
      <description>Price per gallon for fuel oil.</description>
      <type>String</type>
      <units>$/gal</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>auto</default_value>
    </argument>
    <argument>
      <name>prop_rate</name>
      <display_name>Propane: Marginal Rate</display_name>
      <description>Price per gallon for propane.</description>
      <type>String</type>
      <units>$/gal</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>auto</default_value>
    </argument>
    <argument>
      <name>pv_compensation_type</name>
      <display_name>PV: Compensation Type</display_name>
      <description>The type of compensation for PV.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>Net Metering</default_value>
      <choices>
        <choice>
          <value>Net Metering</value>
          <display_name>Net Metering</display_name>
        </choice>
        <choice>
          <value>Feed-In Tariff</value>
          <display_name>Feed-In Tariff</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>pv_sellback_rate</name>
      <display_name>PV: Net Metering Annual Excess Sellback Rate</display_name>
      <description>The annual excess/net sellback rate for PV. Only applies if the PV compensation type is 'Net Metering'.</description>
      <type>String</type>
      <units>$/kWh</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.03</default_value>
    </argument>
    <argument>
      <name>pv_tariff_rate</name>
      <display_name>PV: Feed-In Tariff Rate</display_name>
      <description>The annual full/gross tariff rate for PV. Only applies if the PV compensation type is 'Feed-In Tariff'.</description>
      <type>String</type>
      <units>$/kWh</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.12</default_value>
    </argument>
    <argument>
      <name>enduse_timeseries</name>
      <display_name>Output: File Path</display_name>
      <description>Absolute (or relative) directory to timeseries output.</description>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value></default_value>
    </argument>
  </arguments>
  <outputs>
    <output>
      <name>electricity</name>
      <display_name>electricity</display_name>
      <short_name>electricity</short_name>
      <type>String</type>
      <model_dependent>false</model_dependent>
    </output>
    <output>
      <name>natural_gas</name>
      <display_name>natural_gas</display_name>
      <short_name>natural_gas</short_name>
      <type>Double</type>
      <model_dependent>false</model_dependent>
    </output>
    <output>
      <name>propane</name>
      <display_name>propane</display_name>
      <short_name>propane</short_name>
      <type>Double</type>
      <model_dependent>false</model_dependent>
    </output>
    <output>
      <name>fuel_oil</name>
      <display_name>fuel_oil</display_name>
      <short_name>fuel_oil</short_name>
      <type>Double</type>
      <model_dependent>false</model_dependent>
    </output>
  </outputs>
