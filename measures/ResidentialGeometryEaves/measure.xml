      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>truss, cantilever</default_value>
      <choices>
        <choice>
          <value>truss, cantilever</value>
          <display_name>truss, cantilever</display_name>
        </choice>
        <choice>
